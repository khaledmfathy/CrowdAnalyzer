{
    "log": {
        "level": "silly",
        "maxsize": "5m",
        "maxFiles": "14d",
        "datePattern": "YYYY-MM-DD",
        "zippedArchive": true,
        "handleExceptions": true
    },
    "server": {
        "environment": "production",
        "port": "7002"
    },
    "aws": {
        "region": "us-east-2",
        "sqs":{
            "apiVersion": "2012-11-05",
            "QueueName": "default",
            "Attributes": {
                "DelaySeconds": "60",
                "MessageRetentionPeriod": "86400"
            }
        }
    }
}